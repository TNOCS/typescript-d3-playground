{
  "extends": "tslint:recommended",
  "rules": {
    "quotemark": [true, "single", "double"],
    "max-line-length": [200],
    "whitespace": [true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-separator",
      "check-type"
    ],
    "trailing-comma": [true, {
      "multiline": "never",
      "singleline": "never"
    }],
    "object-literal-sort-keys": false
  }
}